url: <%= url %>
baseurl: /<%= name %>/

timezone: America/Chicago

course:
  number: <%= number %>
  title: <%= title %>
  uid: <%= name %>
  tagline: >
    Write a course tagline in `_config.yml`
  term: <%= anchor_date.year %>
  # Set `days: ""` (empty string) for weekly/asynchronous classes
  days: <%= '""' unless days.length > 0 %>
    <% days.each do |d| %>- <%= d %>
    <% end %>
<% if cc %>
  license:
    name: Creative Commons
    url: https://creativecommons.org/licenses/by/4.0/
<% end %>
  instructor:
    honorific: Dr.
    name: <%= instructor %>
    title: Professor
    full_title: Enter a full professional title in `_config.yml`
    url: https://example.com/instructor/
    github: https://github.com/example/
    email: <%= email %>
    office_hours: >
      Detail your office hours in `_config.yml`
  ta:
    name:
    full_title:
    url:
    email:
    office_hours: >
  navigation:
    - label: Calendar
      href: "#this-week"
      id: cal
    - label: Projects
      href: "projects/#current"
      id: pro
    - label: Policies
      href: "policies/"
      id: pol
  css:
    font-url: "https://use.typekit.net/qqk1feg.css"

collections_dir: syllabus

collections:
  weeks:
  projects:
  policies:
    # Rearrange the files listed here to change their order on the policies page.
    # To remove a section from the policies page, you *must* delete the file in the
    # /syllabus/_policies/ directory
    order:
      - description.md
      - goals.md
      - outcomes.md
      - objectives.md
      - books.md
      - materials.md
      - special-needs.md
      - participation.md
      - assignment-submission.md
      - late-work.md
      - grading-policy.md
      - grading-criteria.md
      - technology-policy.md
      - academic-integrity.md

defaults:
  - scope:
      path: ""
    values:
      layout: "default"

theme: jekyll-theme-open-course

exclude:
  - backstop_data
  - backstop.json
  - exe
  - lib
  - Rakefile
  - "*.gemspec"
